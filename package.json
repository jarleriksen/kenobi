{
  "name": "kenobi-ui",
  "version": "0.1.3",
  "author": "Plateo.io",
  "contributors": [
    {
      "name": "Jarl Eriksen",
      "email": "jarl@plateo.io"
    }
  ],
  "repository": "jarleriksen/kenobi",
  "license": "MIT",
  "main": "commonjs/index.js",
  "module": "esm/index.js",
  "files": [
    "commonjs",
    "esm",
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "dependencies": {
    "@blueprintjs/icons": "^3.9.1",
    "@types/styled-components": "^4.1.0",
    "palx": "^1.0.3",
    "react-focus-lock": "^2.0.4",
    "react-popper": "^1.3.3",
    "react-remove-scroll": "^2.0.4",
    "react-select": "^3.0.4",
    "react-transition-group": "^4.2.1"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir commonjs --extensions \".js,.ts,.tsx\" --ignore '**/stories','**/tests' --source-maps inline",
    "build:esm": "BABEL_ENV=esm babel src --out-dir esm --extensions \".js,.ts,.tsx\" --ignore '**/stories','**/tests' --source-maps inline",
    "build": "concurrently --names 'commonjs,esm,types' 'npm run build:commonjs' 'npm run build:esm' 'npm run build:types'",
    "build-storybook": "build-storybook",
    "dev": "start-storybook -p 6006",
    "docz:deploy": "netlify deploy --dir=.docz/dist --prod"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@reach/component-component": "^0.1.3",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-options": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.5",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "styled-components": "^4.3.2",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1"
  },
  "jest": {
    "testRegex": ".+\\.test\\.js$",
    "testURL": "http://localhost"
  }
}
